<!DOCTYPE html>
<html>
<head>
  <title>Admin Panel</title>
  <style>
    table{
      border: 2px solid black;
    }
    td,th{
      border-right: 2px solid black;
      padding: 5px 20px 5px 20px;
      border-bottom: 2px solid black;
    }
  </style>
</head>
<body>
  <h1>Admin Panel</h1>
  <table>
    <tr>
      <th>Username</th>
      <th>Status</th>
      <th>Credit Points</th>
    </tr>
    <% users.forEach(user => { %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.status %></td>
        <td><%= user.creditPoints %></td>
      </tr>
    <% }); %>
  </table>
</body>
</html>
